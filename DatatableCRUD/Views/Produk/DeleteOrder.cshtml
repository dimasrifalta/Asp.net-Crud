
@model DatatableCRUD.Models.order

<h2>Delete Employee</h2>
@using (Html.BeginForm("DeleteOrder", "Produk", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(a => a.Idorder)
    <div class="form-group">
        <label>First Name</label>
        <p>@Model.Idorder</p>
    </div>

    <div class="form-group">
        <label>Last Name</label>
        <p>@Model.EmployeeId</p>
    </div>

    <div class="form-group">
        <label>Email</label>
        <p>@Model.deskripsi</p>
    </div>

   

    <div>
        <input type="submit" value="Delete" />
    </div>
}